{
  "name": "danet-portal-backend",
  "version": "1.0.0",
  "private": true,
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}"
    ],
    "coveragePathIgnorePatterns": [],
    "coverageThreshold": {
      "global": {
        "branches": 0,
        "functions": 0,
        "lines": 0,
        "statements": 0
      }
    },
    "coverageReporters": [
      "text",
      "html"
    ]
  },
  "scripts": {
    "start": "sls offline",
    "test": "jest --coverage",
    "deploy": "sls deploy"
  },
  "dependencies": {
    "dotenv": "^8.2.0",
    "jsonwebtoken": "^8.5.1",
    "jwks-rsa": "^1.12.2",
    "middy": "^0.36.0",
    "util": "^0.12.3"
  },
  "devDependencies": {
    "jest": "^26.6.3",
    "serverless-dotenv-plugin": "^3.1.0",
    "serverless-offline": "^6.8.0"
  }
}
